<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-lreqpcr-sample_overview-SampleOverviewAction.instance">
                <attr name="component" methodvalue="org.lreqpcr.sample_overview.SampleOverviewTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.lreqpcr.sample_overview.Bundle#CTL_SampleOverviewAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="SampleOverviewAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-lreqpcr-sample_overview-SampleOverviewAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="SampleOverviewTopComponent.settings" url="SampleOverviewTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="properties">
                <file name="SampleOverviewTopComponent.wstcref" url="SampleOverviewTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
